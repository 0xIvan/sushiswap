{
  "name": "@sushiswap/graph-client",
  "version": "0.2.3",
  "description": "Sushi Graph Client",
  "keywords": [
    "sushi",
    "graph",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/graph-client"
  },
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "default": "./.graphclient/index.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "graphclient build",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo .graphclient node_modules",
    "dev": "graphclient serve-dev", 
    "lint": "TIMING=1 eslint . --fix",
    "prepublishOnly": "pnpm build",
    "start": "graphclient serve-prod",
    "validate": "graphclient validate"
  },
  "jest": {
    "preset": "@sushiswap/jest-config/node"
  },
  "dependencies": {
    "@sushiswap/validate": "workspace:*",
    "date-fns": "2.29.3",
    "ethers": "5.7.2",
    "graphql": "16.6.0",
    "node-fetch": "3.3.0",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@graphprotocol/client-add-source-name": "1.0.18",
    "@graphprotocol/client-auto-pagination": "1.1.16",
    "@graphprotocol/client-auto-type-merging": "1.0.23",
    "@graphprotocol/client-cli": "2.2.20",
    "@graphql-mesh/transform-filter-schema": "latest",
    "@graphql-mesh/transform-prefix": "latest",
    "@graphql-mesh/transform-prune": "latest",
    "@graphql-mesh/transform-rename": "latest",
    "@graphql-mesh/transform-type-merging": "latest",
    "@graphql-tools/merge": "latest",
    "@sushiswap/chain": "workspace:*",
    "@sushiswap/eslint-config": "workspace:*",
    "@sushiswap/graph-config": "workspace:*",
    "@sushiswap/jest-config": "workspace:*",
    "@sushiswap/prettier-config": "workspace:*",
    "@sushiswap/typescript-config": "workspace:*",
    "@sushiswap/wagmi-config": "workspace:*",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.0.0",
    "@types/numeral": "2.0.2",
    "@wagmi/core": "0.10.1-cjs",
    "eslint": "8.29.0",
    "jest": "29.3.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "@sushiswap/chain": "*",
    "@wagmi/core": "*"
  },
  "peerDependenciesMeta": {
    "@sushiswap/chain": {
      "optional": false
    },
    "@wagmi/core": {
      "optional": false
    }
  }
}
