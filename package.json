{
  "name": "sushi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "build-analytics-app": "dotenv -- turbo run build --filter=analytics[HEAD^1]",
    "build-apps": "dotenv -- turbo run build --filter={./apps/*}[HEAD^1]... --filter=!dojo --filter=!earn13 --filter=!bridge --filter=!swap13 --filter=!furo",
    "build-apps2": "dotenv -- turbo run build --filter={./apps/*}... --filter=!dojo --filter=!earn13 --filter=!bridge --filter=!swap13 --filter=!furo",
    "build-blog-app": "dotenv -- turbo run build --filter=blog[HEAD^1]",
    "build-earn-app": "dotenv -- turbo run build --filter=earn[HEAD^1]",
    "build-furo-app": "dotenv -- turbo run build --filter=furo[HEAD^1]",
    "build-packages": "dotenv -- turbo run build --filter={./packages/*}[HEAD^1]...",
    "build-protocols": "dotenv -- turbo run build --filter={./protocols/*}[HEAD^1]...",
    "build-root-app": "dotenv -- turbo run build --filter=root[HEAD^1]",
    "build-swap-app": "dotenv -- turbo run build --filter=swap[HEAD^1]",
    "build-xswap-app": "dotenv -- turbo run build --filter=xswap[HEAD^1]",
    "build:ci": "turbo run build --filter=[HEAD^1]",
    "changeset": "changeset",
    "changeset-publish": "changeset publish",
    "changeset-version": "changeset version",
    "check": "turbo run check --no-cache --parallel --continue",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "dotenv -- turbo run dev --no-cache --parallel --continue",
    "disable-remote-caching": "turbo unlink && rm -rf ./node_modules/.cache/turbo",
    "enable-remote-caching": "turbo link",
    "formatold": "prettier --write \"**/*.{ts,tsx,md,js,mjs}\"",
    "format": "prettier --write .",
    "generate": "dotenv -- turbo run generate",
    "generate:ci": "dotenv -- turbo run generate --filter=[HEAD^1]",
    "lint": "turbo run lint --parallel",
    "lint-apps": "turbo run lint --filter={./apps/*}[HEAD^1]...",
    "lint-packages": "turbo run lint --filter={./packages/*}[HEAD^1]...",
    "lint-protocols": "turbo run lint --filter={./protocols/*}[HEAD^1]...",
    "lint:ci": "turbo run lint --parallel --filter=[HEAD^1]",
    "publish-packages": "turbo run build --filter=./packages/* && changeset version && changeset publish",
    "sort": "sort-package-json 'package.json' 'apis/*/package.json' 'apps/*/package.json' 'examples/*/package.json' 'packages/{,!(node_modules)/}**/package.json' 'protocols/{,!(node_modules)/}**/package.json'",
    "sort-check": "sort-package-json 'package.json' 'apis/*/package.json' 'apps/*/package.json' 'examples/*/package.json' 'packages/{,!(node_modules)/}**/package.json' 'protocols/{,!(node_modules)/}**/package.json' --check",
    "test": "dotenv -- turbo run test --parallel",
    "test-apps": "dotenv -- turbo run test --filter={./apps/*}[HEAD^1]...",
    "test-packages": "dotenv -- turbo run test --filter={./packages/*}[HEAD^1]...",
    "test-protocols": "dotenv -- turbo run test --filter={./protocols/*}[HEAD^1]...",
    "test-swap-app": "dotenv -- turbo run test --filter=swap",
    "test:ci": "dotenv -- turbo run test --parallel --filter=[HEAD^1]",
    "version-packages": "dotenv -- turbo run build --filter=./packages/* && changeset version",
    "prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@changesets/cli": "2.25.2",
    "@sushiswap/eslint-config": "workspace:*",
    "@sushiswap/jest-config": "workspace:*",
    "@sushiswap/prettier-config": "workspace:*",
    "@sushiswap/typescript-config": "workspace:*",
    "@types/jest": "29.2.4",
    "@types/node": "^18.11.12",
    "@graphql-eslint/eslint-plugin": "3.14.3",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "dotenv-cli": "6.0.0",
    "jest": "^29.3.0",
    "prettier": "2.8.1",
    "husky": "^8.0.0",
    "is-ci": "3.0.1",
    "sort-package-json": "2.1.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "turbo": "1.6.3"
  },
  "packageManager": "pnpm@7.9.5",
  "engines": {
    "node": ">=16.x",
    "pnpm": ">=7.x"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  },
  "pnpm": {
    "overrides": {
      "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
      "graphql": "16.6.0",
      "typescript": "4.8.2"
    }
  }
}
