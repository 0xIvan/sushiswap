{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"],
      "env": [
        "ACADEMY_URL",
        "ANALYTICS_URL",
        "ALCHEMY_ID",
        "BLOG_URL",
        "EARN_URL",
        "FURO_URL",
        "PARTNER_LIST_PR_WEBHOOK_URL",
        "PARTNER_OCTOKIT_KEY",
        "SWAP_API_V0_BASE_URL",
        "POOLS_API_V0_BASE_URL",
        "OCTOKIT_KEY",
        "TOKENS_API_V0_BASE_URL",
        "NEXT_TELEMETRY_DISABLED",
        "DISCOURSE_API_KEY",
        "ZAPPER_API_KEY",
        "NOTION_API_KEY"
      ]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["build"],
      "env": ["CHAIN_ID", "ANVIL_FORK_URL", "ANVIL_BLOCK_NUMBER", "ALCHEMY_ID", "TEST"]
    },
    "check": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
