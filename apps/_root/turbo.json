{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"],
      "env": [
        "ACADEMY_URL",
        "ANALYTICS_URL",
        "ALCHEMY_ID",
        "BLOG_URL",
        "EARN_URL",
        "FURO_URL",
        "TOKEN_LIST_PR_WEBHOOK_URL",
        "OCTOKIT_KEY",
        "SWAP_API_V0_BASE_URL",
        "POOLS_API_V0_BASE_URL",
        "TOKENS_API_V0_BASE_URL",
        "NEXT_TELEMETRY_DISABLED",
        "TEST_WALLET_INDEX"
      ]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["build"],
      "env": [
        "APP_ENV",
        "CHAIN_ID",
        "ANVIL_FORK_URL",
        "ANVIL_BLOCK_NUMBER",
        "ANVIL_PORT",
        "PLAYWRIGHT_URL",
        "ALCHEMY_ID",
        "TEST",
        "TEST_WALLET_INDEX",
        "NEXT_PUBLIC_TEST_WALLET_INDEX",
        "ANVIL_RPC_URL",
        "NEXT_PUBLIC_ANVIL_RPC_URL"
      ]
    },
    "check": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
