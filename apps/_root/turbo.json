{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": [
        ".next/**"
      ],
      "dependsOn": ["^build"],
      "env": [
        "ACADEMY_URL",
        "ANALYTICS_URL",
        "ALCHEMY_ID",
        "BLOG_URL",
        "EARN_URL",
        "FURO_URL",
        "PARTNER_LIST_PR_WEBHOOK_URL",
        "PARTNER_OCTOKIT_KEY",
        "SWAP_API_V0_BASE_URL"
      ]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["build"],
      "env": [
        "CHAIN_ID",
        "ANVIL_FORK_URL",
        "ANVIL_BLOCK_NUMBER",
        "ALCHEMY_ID",
        "TEST"
      ]
    },
    "check": {
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
