{
  "name": "earn-script",
  "version": "0.0.0",
  "private": true,
  "description": "Prisma",
  "keywords": [
    "earn",
    "script"
  ],
  "homepage": "https://www.sushi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "scripts/earn"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "server": "ts-node src/server.ts",
    "incentives": "ts-node --swc src/incentives.ts",
    "pools": "ts-node --swc src/pools.ts",
    "volume": "ts-node --swc src/volume.ts",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "generate": "graphclient build --fileType js && prisma generate",
    "lint": "TIMING=1 eslint src --fix",
    "test": "jest --passWithNoTests",
    "validate": "graphclient validate"
  },
  "jest": {
    "preset": "@sushiswap/jest-config/node"
  },
  "dependencies": {
    "@sushiswap/abi": "workspace:*",
    "@sushiswap/database": "workspace:*",
    "@sushiswap/chain": "workspace:*",
