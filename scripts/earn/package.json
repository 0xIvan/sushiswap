    "@types/express": "^4.17.13",
    "@whatwg-node/fetch": "latest",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "ethers": "^5.7.0",
    "graphql": "16.6.0",
    "hardhat": "^2.10.0",
    "jest": "^28.1.0",
    "prettier": "^2.7.0",
    "prisma": "^4.6.1",
    "ts-node": "^10.9.0",
    "tsup": "^6.2.0",
    "typescript": "4.8.2"
